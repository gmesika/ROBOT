	<Task corePoolSize="10" maxThreads="100" />
	<!-- Add server connection information -->
	<!-- Add login handling configuration -->
	<InitParam>
		<ParamName>userparam</ParamName>
		<ParamValue>userparamvalue</ParamValue>
		<Description>User Param</Description>
	</InitParam>
	<InitParam>
		<ParamName>gridDefaultMaxNumRequested</ParamName>
		<ParamValue>200</ParamValue>
		<Description>Default Maximum Number of results for search grid</Description>
	</InitParam>
	<InitParam>
        <ParamName>uifShowEditableComboBoxAsSelfInGrid</ParamName>
        <ParamValue>true</ParamValue>
        <Description>Flag that indicates whether combobox control should be rendered in the grid as a combobox or
                           or label, textfield, textarea following the same rules as other controls.
        </Description>
    </InitParam>
    <InitParam>
        <ParamName>uifPersistWorkareaWindowPreferences</ParamName>
        <ParamValue>false</ParamValue>
        <Description>Controls whether workarea form sizes and locations are stored. When true, such preferences are utilized the next time the form is opened in the workarea.  When false, these window preferences are not stored.
                 The default value is true.</Description>
    </InitParam>
    <InitParam>
        <ParamName>uifInitialWorkareaFormSizeFull</ParamName>
        <ParamValue>true</ParamValue>
        <Description>Controls whether a form's initial restored size fills the workarea.
            If workarea form sizes are persisted (see uifPersistWorkareaWindowPreferences),
            the initial size applies only to the very first time a form is opened. Otherwise, initial size applies each time a form is opened.
            When true, its initial size in restore state fills the workarea.  
            When false, its initial size in restore state depends on its content, but 
            is at least 1/2 the workarea width and height.  The default value is false.</Description>
    </InitParam>
    <InitParam>
	<ParamName>isIntegratedWithCSMOnly</ParamName>
	<ParamValue>false</ParamValue>
	<Description>if in integrated env with CSM but not with CRM this flag value should be true</Description>
    </InitParam>
    <InitParam>
    	<ParamName>TestTool_SmartInspector</ParamName>
    	<ParamValue>true</ParamValue>
    </InitParam>
  <!--<TitleBarDisplay value="Breadcrumb"/>-->
  <TitleBarDisplay value=""/>
</uifwks:Workspace>